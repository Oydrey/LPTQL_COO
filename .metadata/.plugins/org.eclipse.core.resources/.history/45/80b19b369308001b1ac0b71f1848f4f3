package version3;

public class Guitar {

	private String serialNumber;
	private double price;
	
	public Guitar (String serialNumber, double price) {
		this.serialNumber = serialNumber;
		this.price = price;
	}

	public String getSerialNumber() {
		return serialNumber;
	}
	
	public double getPrice() {
		return price;
	}
	
}
